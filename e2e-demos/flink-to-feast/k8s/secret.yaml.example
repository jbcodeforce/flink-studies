apiVersion: v1
kind: Secret
metadata:
  name: kafka-feast-consumer-secrets
  labels:
    app: kafka-feast-consumer
type: Opaque
stringData:
  # Schema Registry basic auth (format: username:password)
  # Uncomment and set if Schema Registry requires authentication
  # schema.registry.auth: "username:password"
  
  # Kafka SASL credentials (if using SASL authentication)
  # Uncomment and set if Kafka requires SASL authentication
  # kafka.sasl.username: "your-username"
  # kafka.sasl.password: "your-password"
