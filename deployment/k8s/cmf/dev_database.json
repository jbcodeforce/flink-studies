{
    "apiVersion": "cmf.confluent.io/v1",
    "kind": "KafkaDatabase",
    "metadata": {
      "name": "kafka-db"
    },
    "spec": {
      "kafkaCluster": {
        "connectionConfig": {
          "bootstrap.servers": "kafka.confluent.svc.cluster.local:9071"
        }
      },
      "alterEnvironments": [
        "dev-env", "staging"
      ]
    }
  }