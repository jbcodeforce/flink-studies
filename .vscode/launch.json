{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug RentalEventsProcessing",
            "request": "launch",
            "mainClass": "rental.demo.table.RentalEventsProcessing",
            "projectName": "rental-dsp",
            "env": {
                "KAFKA_BOOTSTRAP_SERVERS": "localhost:9092",
                "CONSUMER_GROUP": "raw-jobs-consumer-debug",
                "RAW_JOBS_TOPIC": "raw-jobs",
                "RAW_ORDERS_TOPIC": "raw-orders",
                "ORDER_DETAILS_TOPIC": "order-details",
                "CATALOG_NAME": "rental",
                "CATALOG_DATABASE_NAME": "rentaldb"
            },
            "vmArgs": "-Dlog4j.configurationFile=log4j2.properties",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false
        }
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "type": "java",
            "name": "Launch ProfitAverageMR",
            "request": "launch",
            "mainClass": "jbcodeforce.datastream.ProfitAverageMR",
            "projectName": "my-flink",
            "args": "--input data/avg.txt"
        },
        {
            "type": "java",
            "name": "Launch LeftOuterJoin",
            "request": "launch",
            "mainClass": "jbcodeforce.p1.LeftOuterJoin",
            "projectName": "my-flink"
        },
        {
            "type": "java",
            "name": "Debug (Launch)",
            "request": "launch",
            "mainClass": "jbcodeforce.p1.WordCountMain",
            "args": "wc.txt"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-RideCountExample<common>",
            "request": "launch",
            "mainClass": "org.apache.flink.training.examples.ridecount.RideCountExample",
            "projectName": "common"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-HourlyTipsExercise<hourly-tips>",
            "request": "launch",
            "mainClass": "org.apache.flink.training.exercises.hourlytips.HourlyTipsExercise",
            "projectName": "hourly-tips"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-HourlyTipsSolution<hourly-tips>",
            "request": "launch",
            "mainClass": "org.apache.flink.training.solutions.hourlytips.HourlyTipsSolution",
            "projectName": "hourly-tips"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-RideCleansingExercise<ride-cleansing>",
            "request": "launch",
            "mainClass": "org.apache.flink.training.exercises.ridecleansing.RideCleansingExercise",
            "projectName": "ride-cleansing"
        }
    ]
}